{% if tagSelectedLibelle is defined %}
{% else %}
    {% set tagSelectedLibelle = "" %}
{% endif %}
<p class="aside_title">Filtrer par tag</p>
{% for tag in tags %}
    <div class=" col-md-4">
        <a href="{{ path('articlesByTag', { 'tagLibelle': tag.title }) }}">
            <span {{ (tag.title == tagSelectedLibelle) ? ' class="label label-primary"' }}{{ (tag.title !=tagSelectedLibelle) ? ' class="label label-default"' }}>
                {{ tag.title}}
            </span>
        </a>
    </div>
{% endfor %}
<br>
<hr>