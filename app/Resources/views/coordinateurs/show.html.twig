{% extends 'layout.html.twig' %}

{% block title %}
    {{ parent() }} - coordinateur
{% endblock %}

{% block body %}

    <header class="panel-heading">
        <h2> Coordinateurs</h2>
    </header>
    <div class="panel-body">

        <form class="form form-vertical">
            <fieldset>            <div class="control-group">
                    <label class="col-sm-2 control-label">Id</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.id }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Nom</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.nom }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Codepostal</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.codepostal }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Ville</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.ville }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Adr1</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.adr1 }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Adr2</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.adr2 }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Adr3</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.adr3 }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Tel</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.tel }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Mobile</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.mobile }}</span>
                    </div>
                </div>            <div class="control-group">
                    <label class="col-sm-2 control-label">Email</label>
                    <div class="col-sm-10">
                        <span class="input-xlarge uneditable-input">{{ coordinateur.email }}</span>
                    </div>
                </div>        </fieldset>
        </form>
        <hr>
    </div>
    <hr>

{% endblock %}

